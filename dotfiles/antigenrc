antigen use oh-my-zsh
antigen bundles <<EOBUNDLES

    # my bundles
    holgiosalos/zsh-bootstrap --branch=safefleet bundles/p10k
    holgiosalos/zsh-bootstrap --branch=safefleet bundles/tmux
    holgiosalos/zsh-bootstrap --branch=safefleet bundles/fzf
    holgiosalos/zsh-bootstrap --branch=safefleet bundles/safefleet

    # from oh-my-zsh's library
    git
    tmux
    autojump
    fzf
    github

    ## just for java development
    # jenv
    # mvn

    # from zsh-users
    zsh-users/zsh-syntax-highlighting
    zsh-users/zsh-autosuggestions
    zsh-users/zsh-completions

EOBUNDLES

antigen theme romkatv/powerlevel10k
antigen apply
